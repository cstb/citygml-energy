  </complexType>
  <complexType name="MechanicalVentilationPropertyType">
    <sequence>
      <element ref="energy:MechanicalVentilation" minOccurs="0" />
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup" />
  </complexType>
  <!--=========================================================================================-->
  <!--MediumTypeValue-->
