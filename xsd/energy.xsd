      </element>
      <element name="dailySchedule" maxOccurs="unbounded" type="energy:DailySchedulePropertyType" />
    </sequence>
  </complexType>
  <complexType name="PeriodOfYearPropertyType">
    <sequence>
      <element ref="energy:PeriodOfYear" minOccurs="0" />
    </sequence>
  </complexType>
  <!--=========================================================================================-->
  <!--PhotovoltaicSystem-->
